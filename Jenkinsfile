pipeline 
{
agent any
stages{
   stage("Working with conditions"){
     steps{
      script{
        
        for (i=1;i<5;i++){

             println "Here is your for loop value :  ${i}"
            
           }
      
           for (ravi in 20..30)
            {
             println "Here is your second for loop value :  ${ravi}"
            }
          }    
       }  
     }
   }
}
